    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".idEingabe" />
        <activity android:name=".optionActivity" />
        <activity android:name=".modification_select" />
        <activity android:name=".alarmActivity" />
        <activity android:name=".continueActivity" />
        <activity android:name=".alarmActivity2" />
        <activity android:name=".confirmationToStart"></activity>
    </application>

</manifest>
